{
  "name": "@wmfs/pg-delta-file",
  "version": "0.0.0-semantically-released",
  "description": "Outputs change-only-update CSV files (or “delta” files) that contain all the necessary actions required to re-synchronize rows in a cloned table.",
  "author": "West Midlands Fire Service",
  "homepage": "https://github.com/wmfs/pg-delta-file#readme",
  "keywords": [
    "tymly",
    "package",
    "etl",
    "pg",
    "postgresql"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wmfs/pg-delta-file.git"
  },
  "bugs": {
    "url": "https://github.com/wmfs/pg-delta-file/issues"
  },
  "main": "./lib/index.js",
  "dependencies": {
    "lodash": "4.17.10",
    "make-dir": "1.3.0",
    "pg": "7.4.3",
    "pg-query-stream": "1.1.1"
  },
  "devDependencies": {
    "chai": "4.1.2",
    "codecov": "3.0.2",
    "cz-conventional-changelog": "2.1.0",
    "mocha": "5.2.0",
    "nyc": "12.0.2",
    "semantic-release": "^15.5.1",
    "standard": "11.0.1",
    "@wmfs/hl-pg-client": "1.1.2"
  },
  "scripts": {
    "lint": "standard",
    "test": "nyc mocha && standard",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {"type": "build", "scope":"deps", "release": "minor"},
        {"type": "build", "scope":"devDeps", "release": "patch"}
      ]
    }
  }
}
